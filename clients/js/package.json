{
  "name": "@solana-program/zk-elgamal-proof",
  "version": "0.0.0",
  "description": "JavaScript client for the ZK ElGamal Proof program",
  "sideEffects": false,
  "module": "./dist/src/index.mjs",
  "main": "./dist/src/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    }
  },
  "files": [
    "./dist/src",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
    "build:docs": "typedoc",
    "test": "tsc --noEmit && ava --serial --timeout=2m",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --check src test",
    "format:fix": "prettier --write src test",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solana-program/zk-elgamal-proof"
  },
  "bugs": {
    "url": "https://github.com/solana-program/zk-elgamal-proof/issues"
  },
  "homepage": "https://github.com/solana-program/zk-elgamal-proof#readme",
  "dependencies": {
    "@solana-program/system": "^0.10.0"
  },
  "peerDependencies": {
    "@solana/kit": "^5.0"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@eslint/js": "^9.39.2",
    "@solana-program/compute-budget": "^0.13.0",
    "@solana-program/record": "^0.1.0",
    "@solana/kit": "^6.0.1",
    "@solana/sysvars": "^6.0.1",
    "@solana/zk-sdk": "0.3.1",
    "@types/node": "^25.2.2",
    "ava": "^6.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "ava": {
    "nodeArguments": [
      "--no-warnings"
    ],
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  },
  "packageManager": "pnpm@9.1.0"
}
