{
  "name": "@solana-program/zk-elgamal-proof",
  "version": "0.1.0",
  "description": "JavaScript client for the ZK ElGamal Proof program",
  "sideEffects": false,
  "module": "./dist/src/index.mjs",
  "main": "./dist/src/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    }
  },
  "files": [
    "./dist/src",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
    "build:docs": "typedoc",
    "test": "ava --serial --timeout=2m",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --check src test",
    "format:fix": "prettier --write src test",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solana-program/zk-elgamal-proof"
  },
  "bugs": {
    "url": "https://github.com/solana-program/zk-elgamal-proof/issues"
  },
  "homepage": "https://github.com/solana-program/zk-elgamal-proof#readme",
  "dependencies": {
    "@solana-program/system": "^0.9.0"
  },
  "peerDependencies": {
    "@solana/kit": "^5.0"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@solana/kit": "^5.0.0",
    "@solana/zk-sdk": "file:../../zk-sdk-wasm-js",
    "@types/node": "^24",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "ava": "^6.1.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.2",
    "typedoc": "^0.28.0",
    "typescript": "^5.5.3"
  },
  "ava": {
    "nodeArguments": [
      "--no-warnings"
    ],
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  },
  "packageManager": "pnpm@9.1.0"
}
